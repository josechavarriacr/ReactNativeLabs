{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-empty": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      ".prettierrc",
      "tsconfig.json"
    ]
  }
}
